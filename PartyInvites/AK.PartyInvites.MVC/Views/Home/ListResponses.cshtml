@model IEnumerable<AK.PartyInvites.BL.Guest>

@{
    Layout = null;
 }
<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
    <title>Responses</title>
</head>
<body>
    <div class="card-body">
    <h2>Here is the list of people attending the party</h2>
    <table class="table table-sm table-striped table-bordered">
    <thead>
        <tr>
        <th>Name</th>
        <th>Email</th>
        <th>PhoneNumber</th>
        </tr>
        </thead>
    <tbody>
        @foreach (AK.PartyInvites.BL.Guest r in Model)
    {
       <tr>
        <td>@r.Name</td>
        <td>@r.Email</td>
        <td>@r.PhoneNumber</td>
       </tr>
    }
    </tbody>
    </table>
  </div>
</body>
</html>

